{
	"name": "stagetec-emberplus-nxct",
	"version": "1.0.1",
	"main": "dist/index.js",
	"scripts": {
		"prepare": "husky",
		"dev": "yarn build:watch",
		"build": "rimraf dist && yarn build:main",
		"build:main": "tsc -p tsconfig.json",
		"build:watch": "tsc -p tsconfig.json --watch",
		"lint:raw": "eslint",
		"lint": "yarn lint:raw .",
		"dist": "yarn companion-module-build",
		"package": "rimraf dist && yarn build:main && yarn dist"
	},
	"license": "MIT",
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write"
		],
		"*.{ts}": [
			"yarn lint:raw --fix"
		]
	},
	"engines": {
		"node": ">=22.13"
	},
	"dependencies": {
		"@companion-module/base": "~1.12.1",
		"es-toolkit": "^1.39.10",
		"node-emberplus": "^3.0.8"
	},
	"devDependencies": {
		"@companion-module/tools": "^2.6.1",
		"@types/node": "^22.18.0",
		"eslint": "^9.34.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"typescript": "~5.9.2",
		"typescript-eslint": "^8.41.0"
	},
	"prettier": "@companion-module/tools/.prettierrc.json",
	"packageManager": "yarn@4.9.4"
}
